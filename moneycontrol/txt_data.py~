# -*- coding: utf-8 -*-

# Define here the models for your scraped items
#
# See documentation in:
# http://doc.scrapy.org/en/latest/topics/items.html

from scrapy.item import Item, Field

class MoneycontrolItem(Item):
    symbol=Field()    
    pe=Field()
    book_value=Field()
    pb=Field()
    pc=Field()
    industry_pe=Field()
    nse_price=Field()
    bse_price=Field()
    sector=Field()
    week_52_high = Field()
    week_52_low = Field()
